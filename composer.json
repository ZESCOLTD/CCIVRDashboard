{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "amirami/livewire-datatables": "dev-master",
        "barryvdh/laravel-dompdf": "^2.2",
        "bensampo/laravel-enum": "4.2.0",
        "diglactic/laravel-breadcrumbs": "^8.0",
        "doctrine/dbal": "^3.5",
        "fruitcake/laravel-cors": "^2.0",
        "google/apiclient": "*",
        "guzzlehttp/guzzle": "^7.9",
        "laravel/framework": "^8.75",
        "laravel/sanctum": "^2.11",
        "laravel/telescope": "^4.10",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^3.4",
        "livewire/livewire": "^2.10",
        "maatwebsite/excel": "^3.1",
        "mtvs/eloquent-approval": "^5.1",
        "pbmedia/laravel-ffmpeg": "^7.8",
        "php-ffmpeg/php-ffmpeg": "^0.19.0",
        "spatie/laravel-medialibrary": "^9.12",
        "spatie/laravel-permission": "^5.11",
        "torann/geoip": "^3.0",
        "yajra/laravel-datatables-oracle": "^9.21",
        "yajra/laravel-oci8": "^8.8"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^5.10",
        "phpunit/phpunit": "^9.5.10",
        "bfinlay/laravel-excel-seeder": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
